source "https://rubygems.org"

# Use the same Jekyll/plugins versions as GitHub Pages
gem "minimal-mistakes-jekyll"
gem "github-pages", group: :jekyll_plugins
gem "jekyll-include-cache", group: :jekyll_plugins


group :jekyll_plugins do
  gem "jekyll-remote-theme"
  gem "jekyll-paginate"
  gem "jekyll-sitemap"
  gem "jekyll-gist"
  gem "jekyll-feed"
end
# If you get a WEBrick error when serving locally, uncomment the next line:
# gem "webrick"
